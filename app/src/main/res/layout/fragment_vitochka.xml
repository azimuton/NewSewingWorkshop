<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/orange"
    tools:context=".fragments.formuls.VitochkaFragment">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:ignore="SpeakableTextPresentCheck">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="1332dp">

                <Button
                    android:id="@+id/btResultA4A9contrVitochka"
                    android:layout_width="0dp"
                    android:layout_height="66dp"
                    android:layout_marginTop="16dp"
                    android:backgroundTint="#BA1919"
                    android:drawableRight="@drawable/calcul"
                    android:fontFamily="@font/dserasercyr1"
                    android:paddingRight="50dp"
                    android:text="Вычислить"
                    android:textSize="20sp"
                    android:textStyle="bold"
                    app:cornerRadius="15dp"
                    app:layout_constraintEnd_toEndOf="@+id/textInputLayout4"
                    app:layout_constraintStart_toStartOf="@+id/textInputLayout4"
                    app:layout_constraintTop_toBottomOf="@+id/textInputLayout4" />

                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/textInputLayout1"
                    style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="16dp"
                    android:layout_marginEnd="16dp"
                    app:boxCornerRadiusBottomEnd="15dp"
                    app:boxCornerRadiusTopStart="15dp"
                    app:boxStrokeColor="#EC1515"
                    app:boxStrokeWidth="2dp"
                    app:counterEnabled="true"
                    app:counterMaxLength="15"
                    app:endIconDrawable="@android:drawable/ic_delete"
                    app:endIconMode="clear_text"
                    app:endIconTint="#D50A0A"
                    app:helperText="Вводимых символов"
                    app:hintTextColor="#DA1717"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/imageView6"
                    app:startIconDrawable="@android:drawable/ic_dialog_dialer">

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/etSbRastvor"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/dserasercyr1"
                        android:hint="Введите полуобхват груди первый"
                        android:maxLength="16"
                        android:textSize="14sp"
                        android:textStyle="bold" />
                </com.google.android.material.textfield.TextInputLayout>

                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/textInputLayout3"
                    style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp"
                    app:boxCornerRadiusBottomEnd="15dp"
                    app:boxCornerRadiusTopStart="15dp"
                    app:boxStrokeColor="#EC1515"
                    app:boxStrokeWidth="2dp"
                    app:counterEnabled="true"
                    app:counterMaxLength="15"
                    app:endIconDrawable="@android:drawable/ic_delete"
                    app:endIconMode="clear_text"
                    app:endIconTint="#D50A0A"
                    app:helperText="Вводимых символов"
                    app:hintTextColor="#DA1717"
                    app:layout_constraintEnd_toEndOf="@+id/btResultVitochka"
                    app:layout_constraintStart_toStartOf="@+id/btResultVitochka"
                    app:layout_constraintTop_toBottomOf="@+id/textView10"
                    app:startIconDrawable="@android:drawable/ic_dialog_dialer">

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/etSg1Vitochka"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/dserasercyr1"
                        android:hint="Введите ширину груди первую"
                        android:maxLength="16"
                        android:textSize="14sp"
                        android:textStyle="bold" />
                </com.google.android.material.textfield.TextInputLayout>

                <Button
                    android:id="@+id/btResultVitochka"
                    android:layout_width="0dp"
                    android:layout_height="66dp"
                    android:layout_marginTop="8dp"
                    android:backgroundTint="#BA1919"
                    android:drawableRight="@drawable/calcul"
                    android:fontFamily="@font/dserasercyr1"
                    android:paddingRight="50dp"
                    android:text="Вычислить"
                    android:textSize="20sp"
                    android:textStyle="bold"
                    app:cornerRadius="15dp"
                    app:layout_constraintEnd_toEndOf="@+id/textInputLayout2"
                    app:layout_constraintStart_toStartOf="@+id/textInputLayout2"
                    app:layout_constraintTop_toBottomOf="@+id/textInputLayout2" />

                <TextView
                    android:id="@+id/textView10"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp"
                    android:text="A4A9 ="
                    android:textColor="@color/black"
                    android:textSize="20sp"
                    android:textStyle="bold"
                    app:layout_constraintStart_toStartOf="@+id/btResultVitochka"
                    app:layout_constraintTop_toBottomOf="@+id/btResultVitochka" />

                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/textInputLayout2"
                    style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    app:boxCornerRadiusBottomEnd="15dp"
                    app:boxCornerRadiusTopStart="15dp"
                    app:boxStrokeColor="#EC1515"
                    app:boxStrokeWidth="2dp"
                    app:counterEnabled="true"
                    app:counterMaxLength="15"
                    app:endIconDrawable="@android:drawable/ic_delete"
                    app:endIconMode="clear_text"
                    app:endIconTint="#D50A0A"
                    app:helperText="Вводимых символов"
                    app:hintTextColor="#DA1717"
                    app:layout_constraintEnd_toEndOf="@+id/textInputLayout1"
                    app:layout_constraintStart_toStartOf="@+id/textInputLayout1"
                    app:layout_constraintTop_toBottomOf="@+id/textInputLayout1"
                    app:startIconDrawable="@android:drawable/ic_dialog_dialer">

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/etPbRastvor"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/dserasercyr1"
                        android:hint="Введите полуобхват груди второй"
                        android:maxLength="16"
                        android:textSize="14sp"
                        android:textStyle="bold" />
                </com.google.android.material.textfield.TextInputLayout>

                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/textInputLayout4"
                    style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    app:boxCornerRadiusBottomEnd="15dp"
                    app:boxCornerRadiusTopStart="15dp"
                    app:boxStrokeColor="#EC1515"
                    app:boxStrokeWidth="2dp"
                    app:counterEnabled="true"
                    app:counterMaxLength="15"
                    app:endIconDrawable="@android:drawable/ic_delete"
                    app:endIconMode="clear_text"
                    app:endIconTint="#D50A0A"
                    app:helperText="Вводимых символов"
                    app:hintTextColor="#DA1717"
                    app:layout_constraintEnd_toEndOf="@+id/textInputLayout3"
                    app:layout_constraintStart_toStartOf="@+id/textInputLayout3"
                    app:layout_constraintTop_toBottomOf="@+id/textInputLayout3"
                    app:startIconDrawable="@android:drawable/ic_dialog_dialer">

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/etSg2Vitochka"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/dserasercyr1"
                        android:hint="Введите ширину груди вторую"
                        android:maxLength="16"
                        android:textSize="14sp"
                        android:textStyle="bold" />
                </com.google.android.material.textfield.TextInputLayout>

                <ImageView
                    android:id="@+id/imageView5"
                    android:layout_width="188dp"
                    android:layout_height="299dp"
                    android:layout_marginTop="8dp"
                    android:src="@drawable/vitochkatwo"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <ImageView
                    android:id="@+id/imageView6"
                    android:layout_width="0dp"
                    android:layout_height="286dp"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="8dp"
                    android:layout_marginEnd="8dp"
                    android:src="@drawable/vitochka"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="1.0"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/imageView5" />

                <TextView
                    android:id="@+id/tvVitochka"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:textColor="@color/black"
                    android:textSize="20sp"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/textView10"
                    app:layout_constraintTop_toTopOf="@+id/textView10"
                    app:layout_constraintVertical_bias="0.0" />

                <TextView
                    android:id="@+id/textView12"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp"
                    android:text="A4A9контр = "
                    android:textColor="@color/black"
                    android:textSize="20sp"
                    android:textStyle="bold"
                    app:layout_constraintStart_toStartOf="@+id/btResultA4A9contrVitochka"
                    app:layout_constraintTop_toBottomOf="@+id/btResultA4A9contrVitochka" />

                <TextView
                    android:id="@+id/tvA4A9contrVitochka"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:textColor="@color/black"
                    android:textSize="20sp"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="@+id/textView12"
                    app:layout_constraintStart_toEndOf="@+id/textView12"
                    app:layout_constraintTop_toTopOf="@+id/textView12" />

            </androidx.constraintlayout.widget.ConstraintLayout>

        </LinearLayout>
    </ScrollView>
</FrameLayout>